@import "./reset.styl";

html,
body,
#app{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  background: #0D47A1;
}

h1{
  text-transform: uppercase;
  color: #FFFFFF;
  font-family: "Helvetica", "Arial";
  font-weight: bold;
  font-size: 50px;
  text-shadow: 1px 1px #1565C0;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
